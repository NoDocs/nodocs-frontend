{
  "hosting": [
    {
      "public": "dist",
      "target": "dev-nodocs-app",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "*public",
          "destination": "/public"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
